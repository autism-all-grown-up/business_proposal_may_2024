	number = {12},
	journaltitle = {Int J Res Rev},
	author = {Rivees, Shabnum and Akhter, Rukhsana and Singh Manhas, Rameshwar},
	urldate = {2024-05-26},
	date = {2022-12-29},
	langid = {english},
	file = {Rivees et al. - 2022 - Prevalence of Autism in Patients Who Reported for .pdf:files/7246/Rivees et al. - 2022 - Prevalence of Autism in Patients Who Reported for .pdf:application/pdf},
}

@article{rodriguez-gomez_systematic_2021,
	title = {A systematic review of common genetic variation and biological pathways in autism spectrum disorder},
	volume = {22},
	issn = {1471-2202},
	url = {https://doi.org/10.1186/s12868-021-00662-z},
	doi = {10.1186/s12868-021-00662-z},
	abstract = {Autism spectrum disorder ({ASD}) is a complex neurodevelopmental condition characterized by persistent deficits in social communication and interaction. Common genetic variation appears to play a key role in the development of this condition. In this systematic review, we describe the relationship between genetic variations and autism. We created a gene dataset of the genes involved in the pathogenesis of autism and performed an over-representation analysis to evaluate the biological functions and molecular pathways that may explain the associations between these variants and the development of {ASD}.},
