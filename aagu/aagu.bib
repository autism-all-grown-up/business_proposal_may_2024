	author = {Rodriguez-Gomez, Diego Alejandro and Garcia-Guaqueta, Danna Paola and Charry-Sánchez, Jesús David and Sarquis-Buitrago, Elias and Blanco, Mariana and Velez-van-Meerbeke, Alberto and Talero-Gutiérrez, Claudia},
