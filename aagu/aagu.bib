	langid = {english},
	keywords = {Autism, Doctor, Evidence-based intervention, General practice, Physician, Primary care},
}

@article{mcdonald_mind_2024,
	title = {Mind the {NIH}-Funding Gap: Structural Discrimination in Physical Health-Related Research for Cognitively Able Autistic Adults},
